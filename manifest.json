{
  "manifest_version": 2,

  "name": "SmartPass",
  "description": "A secure way to log-in to websites using your face",
  "version": "1.0",

  "browser_action": {
    "default_icon": "eye-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}
